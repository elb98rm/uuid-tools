{
    "name": "floor9design/uuid-tools",
    "type": "library",
    "description": "A set of useful uuid tools to extend the `webpatser/laravel-uuid` package",
    "keywords": [
        "floor9design",
        "uuid-tools"
    ],
    "homepage": "https://github.com/floor9design/uuid-tools",
    "license": "MIT",
    "authors": [
        {
            "name": "Rick Morice",
            "email": "rick@floor9design.com",
            "homepage": "http://floor9design.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=7.1",
        "webpatser/laravel-uuid": "3.*",
        "floor9design/machine-identifier": "*"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=5.4.3",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Floor9design\\UuidTools\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Floor9design\\UuidTools\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "providers": [
            "Barryvdh\\Debugbar\\ServiceProvider"
        ],
        "aliases": {
            "Debugbar": "Barryvdh\\Debugbar\\Facade"
        },
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
